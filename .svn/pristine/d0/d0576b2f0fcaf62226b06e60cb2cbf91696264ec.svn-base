
<%@ page contentType="text/html;charset=UTF-8"%>
<%@ include file="/webpage/include/taglib.jsp"%>
<html>
<head>
<%@ include file="/webpage/modules/agentsystem/include/head.jsp"%>
<style type="text/css">
.error {
	color: red;
}

#price-error {
	margin-left: 164px; 
}
#cashPledge-error{
	margin-left: 164px;
}
#cashTra-error{
	margin-left: 164px;
}
#hardwareAccount-error{
	margin-left: 164px;
}
#publicAccount-error{
	margin-left: 164px;
}
#minaAccount-error{
	margin-left: 164px;
}
#vipAccount-error{
	margin-left: 164px;
}
</style>
</head>
<body>
	<div class="row divControl">
		<div class="col-md-12">
			<div class="card">
				<div class="card-body">
					<h3 class="box-title">代理商转移</h3>
					<hr class="m-t-0 m-b-40">
					<form action="#" method="post" style="width: 100%" id="transferForm">
						<input type="hidden" value="${agent.id}" id="id" name="id">
						<div class="form-body">
							<div class="form-group row">
								<label class="control-label text-right col-md-2">当前代理商</label>
								<div class="col-md-9">
									<input type="text" class="form-control" value="${agent.companyName}" readonly>
								</div>
							</div>
							<div class="form-group row">
								<label class="control-label text-right col-md-2">下级代理商</label>
								<div class="col-md-9">
									<select class="form-control custom-select" tabindex="1" name="companyParentId" id="companyParentId">
										<option value="Category">请选择</option>
										<c:forEach items="${agentList}" var="agent">
											<option value="${agent.id}">${agent.companyName}</option>
										</c:forEach>
									</select>
								</div>

							</div>
							<div class="form-group row">
								<label class="control-label text-right col-md-2">硬件余额</label>
								<div class="col-md-9">
									<div class="input-group">
										<input type="text" class="form-control" value="${agent.hardwareAccount }${'元'}"
											readonly>
										<button type="button" id="hardwareBtn" disabled="disabled"
											class="btn waves-effect waves-light btn-info"
											onclick="showAccountModal('${agent.id}','hardwareAccount')">转移</button>
									</div>
								</div>
							</div>
							<div class="form-group row">
								<label class="control-label text-right col-md-2">公众号余额</label>
								<div class="col-md-9">
									<div class="input-group">
										<input type="text" class="form-control" value="${agent.publicAccount }${'元'}"
											readonly>
										<button type="button" id="publicBtn" disabled="disabled"
											class="btn waves-effect waves-light btn-info"
											onclick="showAccountModal('${agent.id}','publicAccount')">转移</button>
									</div>
								</div>

							</div>
							<div class="form-group row">
								<label class="control-label text-right col-md-2">小程序余额</label>
								<div class="col-md-9">
									<div class="input-group">
										<input type="text" class="form-control" value="${agent.minaAccount }${'元'}"
											readonly>
										<button type="button" id="minaBtn" disabled="disabled"
											class="btn waves-effect waves-light btn-info"
											onclick="showAccountModal('${agent.id}','minaAccount')">转移</button>
									</div>
								</div>
							</div>
							<div class="form-group row">
								<label class="control-label text-right col-md-2">VIP余额</label>
								<div class="col-md-9">
									<div class="input-group">
										<input type="text" class="form-control" value="${agent.vipAccount }"
											readonly>
										<button type="button" id="vipBtn" disabled="disabled"
											class="btn waves-effect waves-light btn-info" onclick="showVipModal('${agent.id}')">转移</button>
									</div>
								</div>
							</div>
							<div class="form-group row">
								<label class="col-2 col-form-label"></label>
								<div class="col-9">
									<div class="text-xs-right" style="text-align: right;">
										<button type="button" class="btn btn-default" onclick="jump()">返回</button>
									</div>
								</div>
							</div>
						</div>

					</form>
				</div>

			</div>
		</div>
	</div>
	<!-- 模态框 -->
	<div class="modal fade" id="accountModal" tabindex="-1" role="dialog"
		aria-labelledby="exampleModalLabel" aria-hidden="true"
		style="display: none;">
		<div class="modal-dialog" role="document">
			<div class="modal-content" style="margin-top: 200px; width: 600px">
				<div class="modal-header" style="background-color: #F4F5F9;">
					<h4 class="modal-title" id="title"></h4>
					<button type="button" class="close" data-dismiss="modal"
						aria-label="Close">
						<span aria-hidden="true">×</span>
					</button>
				</div>
				<div class="modal-body">
					<form id="accountForm">
						<input type="hidden" value="" id="agentId" name="id">
						<input type="hidden" value="" id="redoundId" name="redoundId">
						<input type="hidden" value="" id="sign" name="sign"> 
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label"
								style="text-align: center; padding-top: 8px">下级代理商:</label>
							<div class="col-8">
								<input type="text" class="form-control" id="agentName" readonly>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label" 
								style="text-align: center; padding-top: 8px">押金转移:</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="cashPledge"
										id="cashPledge"> <span class="input-group-addon">元</span>
								</div>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label" id="recipient"
								style="text-align: center; padding-top: 8px"></label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="price"
										id="price" > <span class="input-group-addon">元</span>
								</div>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label"
								style="text-align: center; padding-top: 8px">备注:</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="remarks"
										id="remarks">
								</div>
							</div>
						</div>
						<div class="form-group row">
	                	<div class="col-11" >
	                		<div style="float: right">总价格：<span id="totalAccountMoney"></span>元</div>
	                	</div>
	                </div>
					</form>
				</div>

				<div class="modal-footer" style="background-color: #F4F5F9;">
					<button type="button" class="btn waves-effect waves-light btn-info"
						onclick="saveTransferAccount()">确认</button>
				</div>
			</div>
		</div>
	</div>
	<div class="modal fade" id="vipModal" tabindex="-1" role="dialog"
		aria-labelledby="exampleModalLabel" aria-hidden="true"
		style="display: none;">
		<div class="modal-dialog" role="document">
			<div class="modal-content" style="margin-top: 200px; width: 600px">
				<div class="modal-header" style="background-color: #F4F5F9;">
					<h4 class="modal-title" id="exampleModalLabel">VIP账户转移</h4>
					<button type="button" class="close" data-dismiss="modal"
						aria-label="Close">
						<span aria-hidden="true">×</span>
					</button>
				</div>
				<div class="modal-body">
					<form id="vipForm">
						<input type="hidden" value="" id="agentIdA" name="id"> 
						<input type="hidden" value="" id="redoundIdA" name="redoundId">
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label"
								style="text-align: center; padding-top: 8px">下级代理商:</label>
							<div class="col-8">
								<input type="text" class="form-control" id="redoundName"
									value="" readonly>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label" 
								style="text-align: center; padding-top: 8px">押金转移:</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="cashPledge" id="cashTra" >
									<span class="input-group-addon">元</span>
								</div>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label" 
								style="text-align: center; padding-top: 8px">硬件转移:</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="hardwareAccount" id="hardwareAccount" >
									<span class="input-group-addon">元</span>
								</div>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label" 
								style="text-align: center; padding-top: 8px">公众号转移:</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="publicAccount" id="publicAccount" >
									<span class="input-group-addon">元</span>
								</div>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label" 
								style="text-align: center; padding-top: 8px">小程序转移:</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="minaAccount" id="minaAccount" >
									<span class="input-group-addon">元</span>
								</div>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label" 
								style="text-align: center; padding-top: 8px">VIP转移:</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="vipAccount" id="vipAccount" >
									<span class="input-group-addon">元</span>
								</div>
								<span id="vipMoney"></span>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient" class="col-3 control-label"
								style="text-align: center; padding-top: 8px">备注:</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" name="remarks" id="vipRemark">
								</div>
							</div>
						</div>
						<div class="form-group row">
	                	<div class="col-11" >
	                		<div style="float: right">总价格：<span id="totalVipMoney"></span>元</div>
	                	</div>
	                </div>
					</form>
				</div>

				<div class="modal-footer" style="background-color: #F4F5F9;">
					<button type="button" class="btn waves-effect waves-light btn-info"
						onclick="updateTraVipAccount()">确认</button>
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript">
	
		$("document").ready(function() {
			
			$("#accountForm").validate(
					{
						rules : {
							cashPledge:{
								required:true,
								number: true,
								minNumber: $("#cashPledge").val(),
							},
							price:{
								required:true,
								number: true,
								minNumber: $("#price").val(),
							},
							remarks:{
								maxlength: 50
							}
						},
						
						//错误信息提示
						messages:{
							cashPledge:{
								required:"请输入金额",
								number: "请正确输入金额",
								length: "输入数字最多小数点后两位",
								range: "请输入正确的金额范围"
							},
							price:{
								required:"请输入金额",
								number: "请正确输入金额",
								length: "输入数字最多小数点后两位",
								range: "请输入正确的金额范围"
							},
							remarks:{
								maxlength:"最多输入50个字符",
							}
						},
						errorPlacement : function(error, element) {	
			            	error.insertAfter( element.parent().parent()); 		            	         
						}
					});
			 jQuery.validator.addMethod("minNumber",function(value, element){
		            var returnVal = true;
		            inputZ=value;
		            var ArrMen= inputZ.split(".");    //截取字符串
		            if(ArrMen.length==2){
		                if(ArrMen[1].length>2){    //判断小数点后面的字符串长度
		                    returnVal = false;
		                    return false;
		                }
		            }
		            return returnVal;
		        },"小数点后最多为两位"); 
		$("#vipForm").validate(
				{
					rules : {
						cashPledge:{
							required:true,
							number: true,
							minNumber: $("#cashPledge").val(),
							range:[0,'${agent.vipAccount}']
						},
						hardwareAccount:{
							required:true,
							number: true,
							minNumber: $("#price").val(),
							range:[0,'${agent.vipAccount}']
						},
						publicAccount:{
							required:true,
							number: true,
							minNumber: $("#price").val(),
							range:[0,'${agent.vipAccount}']
						},
						minaAccount:{
							required:true,
							number: true,
							minNumber: $("#price").val(),
							range:[0,'${agent.vipAccount}']
						},
						vipAccount:{
							required:true,
							number: true,
							minNumber: $("#price").val(),
							range:[0,'${agent.vipAccount}']
						},
						remarks:{
							maxlength: 50
						}
					},
					
					//错误信息提示
					messages:{
						cashPledge:{
							required:"请输入金额",
							number: "请正确输入金额",
							length: "输入数字最多小数点后两位",
							range: "请输入正确的金额范围"
						},
						hardwareAccount:{
							required:"请输入金额",
							number: "请正确输入金额",
							length: "输入数字最多小数点后两位",
							range: "请输入正确的金额范围"
						},
						publicAccount:{
							required:"请输入金额",
							number: "请正确输入金额",
							length: "输入数字最多小数点后两位",
							range: "请输入正确的金额范围"
						},
						minaAccount:{
							required:"请输入金额",
							number: "请正确输入金额",
							length: "输入数字最多小数点后两位",
							range: "请输入正确的金额范围"
						},
						vipAccount:{
							required:"请输入金额",
							number: "请正确输入金额",
							length: "输入数字最多小数点后两位",
							range: "请输入正确的金额范围"
						},
						remarks:{
							maxlength:"最多输入50个字符",
						}
					},
					errorPlacement : function(error, element) {	
		            	error.insertAfter( element.parent().parent()); 		            	         
					}
				});
	        
		});
		var currentId,lowerName;
 		$("#companyParentId").change(function() {
 			if($(this).val() != "Category") {
 				$("#hardwareBtn").attr("disabled", false); 
 				$("#publicBtn").attr("disabled", false);
 				$("#minaBtn").attr("disabled", false);
 				$("#vipBtn").attr("disabled", false); 
 				currentId = $(this).val();
 				lowerName = $(this).find("option:selected").text();
 			}
		});
		
		function showAccountModal(agentId,sign) {
				$("#accountForm").data("validator").resetForm();
				document.getElementById("accountForm").reset();
				$("#sign").val(sign);
				$("#agentName").val(lowerName);
				$("#redoundId").val(currentId);
				$("#agentId").val(agentId);
				$("#totalAccountMoney").html(0);
				$("#remarks").val("");
				if(sign == "hardwareAccount") {
					$("#title").text("硬件账户转移");
					$("#recipient").html("硬件转移:");
				
					$("#price").rules("add",{
						range: [0,'${agent.hardwareAccount}']	
					});
					$("#cashPledge").rules("add",{
						range: [0,'${agent.hardwareAccount}']	
					});
				}
				if(sign == "publicAccount") {
					$("#title").text("公众号账户转移");
					$("#recipient").html("公众号转移:");
					$("#price").rules("add",{
						range: [0,'${agent.publicAccount}']	
					});
					$("#cashPledge").rules("add",{
						range: [0,'${agent.publicAccount}']	
					});
				}
				if(sign == "minaAccount") {
					$("#title").text("小程序账户转移");
					$("#recipient").html("小程序转移:");
					$("#price").rules("add",{
						range: [0,'${agent.minaAccount}']	
					});
					$("#cashPledge").rules("add",{
						range: [0,'${agent.minaAccount}']	
					});
				}
				
				$("#accountModal").modal("show");	
		}
		var price=0,cashPledge=0;
		$('#price').bind('input propertychange', function() {
			price = $('#price').val();
			$('#totalAccountMoney').html(Number(cashPledge)+Number(price));
		});
		$('#cashPledge').bind('input propertychange', function() {
			cashPledge = $('#cashPledge').val();
			$('#totalAccountMoney').html(Number(cashPledge)+Number(price));
		});
		
		function saveTransferAccount() {
			var isValid = $("#accountForm").valid();
			var flag,isValidate,totalMoney;
			if(isValid) {
				if($("#price").val() == 0 && $("#cashPledge").val() == 0) {
					flag = false;
					layer.msg("您还未输入一个非0的转移金额",{icon:7,time:1500});
				}else {
					flag = true;
				}
				totalMoney = $("#totalAccountMoney").text();
				if($("#sign").val() == 'hardwareAccount') {
					if(Number(totalMoney) > Number('${agent.hardwareAccount}')) {
						layer.msg("您输入的转移金额大于当前硬件账户的余额"+'${agent.hardwareAccount}'+"元",{icon:7,time:1500});
						isValidate = false;
					}else{
						isValidate = true;
					}
				}
				if($("#sign").val() == 'publicAccount') {
					if(Number(totalMoney) > Number('${agent.publicAccount}')) {
						layer.msg("您输入的转移金额大于当前公众号账户的余额"+'${agent.publicAccount}'+"元",{icon:7,time:1500});
						isValidate = false;
					}else{
						isValidate = true;
					}
				}
				if($("#sign").val() == 'minaAccount') {
					if(Number(totalMoney) > Number('${agent.minaAccount}')) {
						layer.msg("您输入的转移金额大于当前小程序账户的余额"+'${agent.minaAccount}'+"元",{icon:7,time:1500});
						isValidate = false;
					}else{
						isValidate = true;
					}
				}
			}
			if(isValid && flag && isValidate) {
				var locked = false;
				layer.confirm('是否确认转移?', {
					skin : 'layui-layer-molv',
					icon : 3,
					btn : [ '确认', '取消' ]
				//按钮
				},function() {
					if(!locked) {
						locked = true;
						$.ajax({
							url : "${ctx}/agentSystem/SysUser/updateTransferAccount",
							type : "post",
							dataType : "json",
							data : $('#accountForm').serialize(),
							success : function(result) {
								if (result == 1) {
									$("#accountModal").modal("hide");
									$("#transferForm").submit();
									layer.msg("转移成功！", {
										icon : 1,
										time : 1000
									});
								
								} else {
									$("#accountModal").modal("hide");
									layer.msg("转移失败！", {
										icon : 1,
										time : 1000
									});
								}
							}
						});
					}
				
				});
			}
		}
		
		function showVipModal(agentId) {
			$("#vipForm").data("validator").resetForm();
			document.getElementById("vipForm").reset();  
			$("#redoundName").val(lowerName);
			$("#redoundIdA").val(currentId);
			$("#agentIdA").val(agentId);		
			$("#totalVipMoney").html(0);
			$("#vipModal").modal("show");
		}
		var cashMoney=0,hardwareMoney=0,publicMoney=0,minaMoney=0,vipMoney=0;
		$('#cashTra').bind('input propertychange', function() {
			cashMoney = $('#cashTra').val();
			$('#totalVipMoney').html(Number(cashMoney)+Number(hardwareMoney)+Number(publicMoney)+Number(minaMoney)+Number(vipMoney));
		});
		$('#hardwareAccount').bind('input propertychange', function() {
			hardwareMoney = $('#hardwareAccount').val();
			$('#totalVipMoney').html(Number(cashMoney)+Number(hardwareMoney)+Number(publicMoney)+Number(minaMoney)+Number(vipMoney));
		});
		$('#publicAccount').bind('input propertychange', function() {
			publicMoney = $('#publicAccount').val();
			$('#totalVipMoney').html(Number(cashMoney)+Number(hardwareMoney)+Number(publicMoney)+Number(minaMoney)+Number(vipMoney));
		});
		$('#minaAccount').bind('input propertychange', function() {
			minaMoney = $('#minaAccount').val();
			$('#totalVipMoney').html(Number(cashMoney)+Number(hardwareMoney)+Number(publicMoney)+Number(minaMoney)+Number(vipMoney));
		});
		$('#vipAccount').bind('input propertychange', function() {
			vipMoney = $('#vipAccount').val();
			$('#totalVipMoney').html(Number(cashMoney)+Number(hardwareMoney)+Number(publicMoney)+Number(minaMoney)+Number(vipMoney));
		});
		function updateTraVipAccount() {
			var isValid = $("#vipForm").valid();
			var totalMoney = $("#totalVipMoney").html();
			if(isValid) {
				if(Number(totalMoney) == 0.00) {
					layer.msg("您还未输入一个非0的转移金额",{icon:7,time:1500});
					return
				}
				if(Number(totalMoney) > Number('${agent.vipAccount}')) {
					layer.msg("您输入的转移金额大于当前VIP账户的余额"+'${agent.vipAccount}'+"元",{icon:7,time:1500});
					return
				}
				var locked = false;
				layer.confirm('是否确认转移?', {
					skin : 'layui-layer-molv',
					icon : 3,
					btn : [ '确认', '取消' ]
				//按钮
				},function() {
					alert($('#vipForm').serialize())
					if(!locked) {
						locked = true;
						$.ajax({
							url : "${ctx}/agentSystem/SysUser/updateTransferVipAccount",
							type : "post",
							dataType : "json",
							data : $('#vipForm').serialize(),
							success : function(result) {
								if (result == 1) {
									$("#vipModal").modal("hide");
									$("#transferForm").submit();
									layer.msg("转移成功！", {
										icon : 1,
										time : 1000
									});
								
								} else {
									$("#vipModal").modal("hide");
									layer.msg("转移失败！", {
										icon : 1,
										time : 1000
									});
								}
							}
						});
					}
				
				});
			}
			
			
		}
		
		function jump() {
			window.location.href = "${ctx}/agentSystem/SysUser/list";
		}
	</script>
</body>
</html>