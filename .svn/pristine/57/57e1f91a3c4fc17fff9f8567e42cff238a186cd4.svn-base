
<%@ page contentType="text/html;charset=UTF-8"%>
<%@ include file="/webpage/include/taglib.jsp"%>
<html>
<head>
<%@ include file="/webpage/modules/agentsystem/include/head.jsp"%>
	<style type="text/css">
		.error {
			color: red;
		}
		select.error{
			color:#67757c;
		}
		#agentin_name-error {
			margin-left: 27%;
		}
		#password-error {
			margin-left: 27%;
		}
		#companyName-error {
			margin-left: 27%;
		}
		#name-error {
			margin-left: 27%;
		}
		#mobile-error {
			margin-left: 27%;
		}
		#address-error {
			margin-left: 27%;
		}
		#customerLevel-error{
			margin-left: 27%;
		}
		#startDateA-error{
			width: 55%;
   			text-align: left;
   			display:inline-block!important;
		}
		#email-error{
			margin-left: 27%;
		}
		#hardwareExpendA-error{
			margin-left: 28%;
		}
		#publicExpendA-error{
			margin-left: 28%;
		}
		#minaExpendA-error{
			margin-left: 28%;
		}
		#AvipExpendA-error{
			margin-left: 28%;
		}
		#theater-error{
			margin-left: 27%;
		}
		#principal-error{
			margin-left: 27%;
		}
		
	</style>
</head>
<body>
	<div class="row divControl">
		<div class="col-md-12">
			<div class="card">
				<div class="card-body">
					<h3 class="box-title">代理商基本信息设置</h3>
					<hr class="m-t-0 m-b-40">
					<form action="#" method="post"
						style="width: 100%" id="addForm">
						<div class="form-body">

							<div class="row">
								<input id="userId" name="id" type="hidden" value="" /> 
								<input id="oldName" name="oldName" type="hidden" value="" /> 
								<input id="oldPassword" name="oldPassword" type="hidden" value="" /> 
								<input  name="operation" type="hidden" value="添加" /> 
								<input id="publicAccountDeduct" name="publicAccountDeduct" type="hidden" value="" /> 
								<input id="minaAccountDeduct" name="minaAccountDeduct" type="hidden" value="" /> 
								<input id="hardwareAccountDeduct" name="hardwareAccountDeduct" type="hidden" value="" /> 
								<input id="vipAccountDeduct" name="vipAccountDeduct" type="hidden" value="" /> 
								<div class="col-md-6">
									<div class="form-group row">

										<label class="col-3 col-form-label"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>账号</label>

										<div class="col-md-9">
											<input type="text" class="form-control height-control" id="agentin_name" name="login_name" maxlength="15">
										</div>
									</div>



									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>公司全称</label>
										<div class="col-md-9">
											<input type="text" class="form-control" placeholder="公司全称" id="companyName" maxlength="30"
												name="companyName">
										</div>
									</div>



									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>联系方式</label>
										<div class="col-md-9">
											<input type="text" class="form-control"
												placeholder="公司负责人或企业法人联系方式" name="mobile">
										</div>
									</div>



									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>公司地址</label>
										<div class="col-md-9">
											<input type="text" class="form-control" placeholder="公司地址" maxlength="50"
												name="address">

										</div>
									</div>

									<div class="form-group row">
										<label class="control-label text-right col-md-3">QQ</label>
										<div class="col-md-9">
											<input type="text" class="form-control" placeholder="负责人QQ" maxlength="20"
												name="qq">
										</div>
									</div>
									
									<shiro:lacksPermission name="agency:wasSysUser:addInside">  
										<div class="form-group row">
											<label class="control-label text-right col-md-3"><span
												style="color: red">*&nbsp;&nbsp;&nbsp;</span>所属战区</label>
											<div class="col-md-9">
												<input type="text" class="form-control"  maxlength="20"
													name="theater" required>
											</div>
										</div>
									</shiro:lacksPermission> 
										
									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>套餐折扣</label>
										<div class="col-md-9">
											<div class="input-group">
												<input type="text" class="form-control"  name="packageDiscount" id="packageDiscountV" maxlength="5"> 
												<span class="input-group-addon">折</span>
											</div>
											<span id="packageDiscount" class="help-block"></span>
										</div>
									</div>

									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>公众号折扣</label>
										<div class="col-md-9">
											<div class="input-group">
												<input type="text" class="form-control"  name="publicDiscount" id="publicDiscountV" maxlength="5"> 
												<span class="input-group-addon">折</span>
											</div>
											<span id="publicDiscount" class="help-block"></span>
										</div>
									</div>
									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>小程序折扣</label>
										<div class="col-md-9">
											<div class="input-group">
												<input type="text" class="form-control"  name="minaDiscount" id="minaDiscountV" maxlength="5">
												<span class="input-group-addon">折</span>
											</div>
											<span id="minaDiscount" class="help-block"></span>
										</div>
									</div>

									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>客户等级</label>
										<div class="col-md-9">
											<input type="radio" class="check"  name="customerLevel" data-radio="iradio_square-blue" value="0" checked>
	                                        <label for="square-radio-A">普通客户</label>
	                                         <input type="radio" class="check"  name="customerLevel" data-radio="iradio_square-blue" value="1">
	                                        <label for="square-radio-B">VIP客户</label>
										</div>
									</div>

									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>时间范围</label>
										<div class="col-md-9">
											<div class="input-daterange input-group" id="date-range">
												<input type="text" class="form-control" name="startDateA">
												<span class="input-group-addon bg-info b-0 text-white">至</span>
												<input type="text" class="form-control" name="endDateA">
											</div>
											<label id="startDateA-error" class="error" for="startDateA" style=""></label>
											<label id="endDateA-error" class="error" for="endDateA" style=""></label>
										</div>

									</div>

									<shiro:lacksPermission name="agency:wasSysUser:addInside">  
										<div class="form-group row">
											<label class="control-label text-right col-md-3"><span
												style="color: red">*&nbsp;&nbsp;&nbsp;</span>押金</label>
											<div class="col-md-9">
												<div class="input-group">
	
													<input type="text" class="form-control" id="cashPledge" name="cashPledge" readonly required>
	
													<button type="button"
														class="btn waves-effect waves-light btn-info" onclick="showModel()">
														扣款方式</button>
												</div>
											</div>
										</div>
									</shiro:lacksPermission> 

								</div>


								<!--/span-->
								<div class="col-md-6">
									<div class="form-group has-danger row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>密码</label>
										<div class="col-md-9">
											<input type="text" maxlength="20"
												class="form-control form-control-danger" name="password">
										</div>
									</div>
									
									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>负责人</label>
										<div class="col-md-9">
											<input type="text" class="form-control" maxlength="10"
												placeholder="公司负责人或企业法人" name="name">
										</div>
									</div>
									
									<div class="form-group row " >
										<label class="control-label text-right col-md-3"><span
											style="color: red;height:27px">*&nbsp;&nbsp;&nbsp;</span>地区:</label>
											<div data-toggle="distpicker" class="col-md-9 area">
													<div class="row" style="height:35px">
													<div class="col-md-4 ">
														<div class="form-group row">
															<div class="col-sm-12 col-xs-12">
																<div>
																	<select class="form-control" id="province" name="province"></select>
																</div>
															</div>
														</div>
													</div>
													<div class="col-md-4">
														<div class="form-group row">
															<div class="col-sm-12 col-xs-12">
																<div>
																	<select class="form-control" id="city" name="city"></select>
																</div>
															</div>
														</div>
													</div>
													<div class="col-md-4">
														<div class="form-group row">
															<div class="col-sm-12 col-xs-12">
																<div>
																	<select class="form-control" id="county" name="county"></select>
																</div>
															</div>
														</div>
													</div>
												</div>
											</div>
									</div>


									<div class="form-group row">
										<label class="control-label text-right col-md-3">邮箱</label>
										<div class="col-md-9">
											<input type="text" class="form-control" placeholder="负责人邮箱" maxlength="30"
												name="email">
										</div>
									</div>
									<div class="form-group row">
										<label class="control-label text-right col-md-3">微信号</label>
										<div class="col-md-9">
											<input type="text" class="form-control" placeholder="负责人微信" maxlength="30"
												name="wechat">
										</div>
									</div>
									
									<shiro:lacksPermission name="agency:wasSysUser:addInside">  
										<div class="form-group row">
											<label class="control-label text-right col-md-3"><span
												style="color: red">*&nbsp;&nbsp;&nbsp;</span>战区负责人</label>
											<div class="col-md-9">
												<input type="text" class="form-control"  maxlength="30"
													name="principal" required>
											</div>
										</div>
									</shiro:lacksPermission> 

									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>所属上级</label>
										<div class="col-md-9">
											
											<div class="editable-input" style="position: relative;">
												<shiro:hasPermission name="agency:wasSysUser:addInside">
													<select class="form-control input-sm" name="companyParentId" id="companyParentId" required>
														<option value="">--请选择--</option>
														<option value="1">无锡微炫客</option>
														<c:forEach items="${agentUser}" var="agent">
															<c:if test="${agent.companyName!='无锡微炫客'}">
																<option value="${agent.id}">${agent.companyName}</option>
															</c:if>
														</c:forEach>
													</select>
												</shiro:hasPermission>  	
											
												<shiro:lacksPermission name="agency:wasSysUser:addInside">  
													<div><input type="text" class="form-control" value="${sysUser.companyName}"  name="companyParentId" readOnly></div>
												</shiro:lacksPermission> 
											</div>

										</div>
									</div>

									<div class="form-group row">
										<label class="control-label text-right col-md-3"><span
											style="color: red">*&nbsp;&nbsp;&nbsp;</span>代理商等级</label>
										<div class="col-md-9">
											<div class="editable-input" style="position: relative;">
												
												<shiro:hasPermission name="agency:wasSysUser:addInside">
													<select class="form-control input-sm selectChange" name="agentLevelIdA">
														<option value="">--请选择--</option>
														<c:forEach items="${allWasAgentLevel}" var="agent">
															<option value="${agent.salesPolicyIds}@${agent.agentLevelId}">${agent.name}</option>
														</c:forEach>
													</select>
												</shiro:hasPermission>  		
												
												<shiro:lacksPermission name="agency:wasSysUser:addInside">  
													<select class="form-control input-sm selectChange" name="agentLevelIdA">
														<option value="">--请选择--</option>
														<c:forEach items="${wasAgentLevel}" var="agent">
															<option value="${agent.salesPolicyIds}@${agent.agentLevelId}">${agent.name}</option>
														</c:forEach>
													</select>
												</shiro:lacksPermission> 
										
											</div>
											<div id="WasSalesPolicys">
											
											</div>
										</div>
									</div>

									<div class="form-group row">
										<label class="control-label text-right col-md-3">证书</label>
										<div class="col-md-9">
											<div class="input-group">
												<input type="text" class="form-control singleGraph height-control rightAngle" name="certificate" id="certificate" readonly>
												<button type="button"
													class="btn btn-default leftAngle height-control" style=" height: 36px;font-size: 12px;" onclick="lopen('upImg','certificate','1')">上传</button>
											</div>
										</div>
									</div>

								</div>
							</div>
						</div>
						<div class="form-group row">
							<label class="col-2 col-form-label"></label>
							<div class="col-9">
								<div class="text-xs-right" style="text-align: right;">
									<button type="button" class="btn btn-default" onclick="jump()">返回</button>
									<button type="button"
										class="btn waves-effect waves-light btn-info" onclick="saveA()">保存</button>
								</div>
							</div>
						</div>
					</form>
				</div>

			</div>
		</div>
	</div>
	
	<!-- 模态框 -->
	<div class="modal fade" id="modalA" tabindex="-1" role="dialog"
		aria-labelledby="exampleModalLabel" aria-hidden="true"
		style="display: none;">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header" style="background-color: #F4F5F9;">
					<h4 class="modal-title" id="exampleModalLabel">扣款方式</h4>
					<button type="button" class="close" data-dismiss="modal"
						aria-label="Close">
						<span aria-hidden="true">×</span>
					</button>
				</div>
				<div class="modal-body">
					<form id="modalFormA">
						<div class="form-group row">
							<label for="recipient11" class="col-3 control-label"
								style="padding-top: 8px">押金扣款金额</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control" id="PledgeConfirmA"
										value="${cashPledge}" readonly> <span
										class="input-group-addon">元</span>
								</div>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient11" class="col-3 control-label"
								style="padding-top: 8px">硬件账户扣款</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control ExpendA" name="hardwareExpendA" id="hardwareExpendA">
									<span class="input-group-addon">元</span>
								</div>
								<span>您的硬件账户余额为${sysUser.hardwareAccount}元</span>
							</div>
						</div>
						<div class="form-group row">
							<label for="recipient12" class="col-3 control-label"
								style="padding-top: 8px">公众号账户扣款</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control ExpendA" name="publicExpendA" id="publicExpendA">
									<span class="input-group-addon">元</span>
								</div>
								<span>您的公众号账户余额为${sysUser.publicAccount}元</span>
							</div>

						</div>
						<div class="form-group row">
							<label for="recipient13" class="col-3 control-label"
								style=" padding-top: 8px">小程序账户扣款</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control ExpendA" name="minaExpendA" id="minaExpendA">
									<span class="input-group-addon">元</span>
								</div>
								<span>您的小程序账户余额为${sysUser.minaAccount}元</span>
							</div>

						</div>
						<div class="form-group row">
							<label for="recipient14" class="col-3 control-label"
								style="padding-top: 8px">VIP账户扣款</label>
							<div class="col-8">
								<div class="input-group">
									<input type="text" class="form-control ExpendA" name="AvipExpendA" id="AvipExpendA">
									<span class="input-group-addon">元</span>
								</div>
								<span>您的VIP账户余额为${sysUser.vipAccount}元</span>
							</div>

						</div>
						
						<div class="form-group row">
	                	<div class="col-11" >
	                		<div style="float: right">总价格：<span id="totalAccountMoney">0</span>元</div>
	                	</div>
	                </div>
						
					</form>
				</div>
				<div class="modal-footer" style="background-color: #F4F5F9;">
					<button type="button" class="btn waves-effect waves-light btn-info" onclick="cashPledgeConfirm()">确定</button>
				</div>
			</div>
		</div>
	</div>
	
	<!-- 模态框2 -->
	<div class="modal fade" id="ModalSure" tabindex="-1" role="dialog"
		aria-labelledby="ModalLabela" aria-hidden="true"
		style="display: none; margin-top: 200px; margin-left: 100PX;">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header" style="background-color: #F4F5F9;">
					<p class="modal-title" id="exampleModalLabela"
						style="font-size: 14px;">信息确认</p >
					<button type="button" class="close" data-dismiss="modal"
						aria-label="Close">
						<span aria-hidden="true" id="closex">×</span>
					</button>
				</div>
				<div class="modal-body">
					<form id="orderForm" action="" style="font-size: 13px;">
						<div class="form-group row">
							<div class="col-6">
								<label>硬件账户扣款：</label> 
								<label for="example-text-input" id="hardwareExpendB"></label>
							</div>
							<div class="col-6">
								<label>公众号账户扣款：</label> 
								<label for="example-text-input" id="publicExpendB"></label>
							</div>
						</div>
						<div class="form-group row">
							<div class="col-6">
								<label>小程序账户扣款：</label> 
								<label for="example-text-input" id="minaExpendB" ></label>
							</div>
							<div class="col-6">
								<label>VIP账户扣款：</label> 
								<label for="example-text-input" id="AvipExpendB" ></label>
							</div>
						</div>
					</form>
				</div>
				<div class="modal-footer" style="background-color: #F4F5F9; border-bottom-left-radius: 5px; border-bottom-right-radius: 5px;">
						<button type="button" class="btn  btn-info" id="sure"  onclick="save()">确认</button>
				</div>
			</div>
		</div>
	</div>
	
	<script src="${ctxStatic}/agentSystem/js/distpicker.data.js"></script>
	<script src="${ctxStatic}/agentSystem/js/distpicker.js"></script>
	<script src="${ctxStatic}/agentSystem/js/main.js"></script>
	<script type="text/javascript">
		$("document").ready(function() {
			//表单验证
			packageDiscountV=0.01;
			publicDiscountV=0.01;  
			minaDiscountV=0.01;
			$("#addForm").validate(
					{
						rules : {
							login_name : {
								required : true,
								maxlength : 15,
								alnum : true,
								 remote:{
									    type:"POST",
									    url:"${ctx}/agentsystem/user/checkName",           
										data:{
											name:function(){return $("#agentin_name").val();},
											operation:"添加"
									      }
										}
							},
							password : {
								required : true,
								minlength : 6,
								maxlength : 20,
								alnum : true
							},
							companyName : {
								required : true,
								maxlength : 30,
									remote:{
									    type:"POST",
									    url:"${ctx}/agency/wasAgentApply/validateCompanyName",           
										data:{
											companyName:function(){return $("#companyName").val();},
											operation:"添加"
									      }
										}
							},
							name : {
								required : true,
								minlength : 2,
								maxlength : 10
							},
							mobile:{
			    				required:true,
			    				isphone:true,
			    			},
			    			province:{
			    				validateArea:true,
			    			},
							address : {
								required : true,
								maxlength : 50
							},
							email : {
								maxlength : 30,
								email : true
							},
							qq :{
